!!!
%html
%head
  %title GcampJordanCollier
  = stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true
  = javascript_include_tag 'application', 'data-turbolinks-track' => true
  = csrf_meta_tags
%body
  %nav.navbar.navbar-inverse.navbar-fixed-top
    .container
      .navbar-header
        %button.navbar-toggle.collapsed{"aria-controls" => "navbar", "aria-expanded" => "false", "data-target" => "#navbar", "data-toggle" => "collapse", :type => "button"}
          %span.sr-only Toggle navigation
          %span.icon-bar
          %span.icon-bar
          %span.icon-bar
        %a.navbar-brand{:href => "#"} gCamp
      #navbar.navbar-collapse.collapse
  .container
    = yield
    %footer
      .row
        .col-md-6
          %a{:href => "about"} About
          |
          %a{:href => "terms"} Terms
          |
          %a{:href => "faq"} FAQ
          |
          %a{:href => "tasks"} Tasks
        .col-md-6.text-right
          %a{:href => "https://www.linkedin.com"}
            = image_tag"linked.jpg", class: "img-footer"
          %a{:href => "https://github.com/"}
            = image_tag"github.png", class: "img-footer"
          %a{:href => "https://twitter.com/"}
            = image_tag"twitter.png", class: "img-footer"
          %a{:href => "https://students.gschool.it/"}
            = image_tag "galvanize.png", class: "img-footer"



  %script{:src => "https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"}
  %script{:src => "../../dist/js/bootstrap.min.js"}
  %script{:src => "../../assets/js/ie10-viewport-bug-workaround.js"}
